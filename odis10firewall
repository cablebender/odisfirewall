new-netfirewallrule -DisplayName '01 Kommunikation zwischen ODIS und VAS6154(A)' -Profile @('Private','Public') -Direction Inbound -Action Allow -Protocol TCP -Remoteport 6154 -Remoteaddres @('192.168.13.69','192.168.13.100-192.168.13.254') -program '%ProgramFiles(x86)%\Volkswagen\VAS6154 Driver\VAS6154App.exe'
new-netfirewallrule -DisplayName '02 DoIP-Kommunikation zwischen ODIS Service und VAS6154(A)' -Profile @('Private','Public') -Direction Inbound -Action Allow -Protocol TCP -Remoteport @('4500','13400') -Remoteaddres @('192.168.13.69','192.168.13.100-192.168.13.254') -program '%ProgramFiles%\Offboard_Diagnostic_Information_System_Service\OffboardDiagLauncher.exe'
new-netfirewallrule -DisplayName '03 DoIP-Kommunikation zwischen ODIS Service und VAS6154(A)' -Profile @('Private','Public') -Direction Inbound -Action Allow -Protocol UDP -Remoteport @('4500','13400') -Remoteaddres @('192.168.13.69','192.168.13.100-192.168.13.254') -program '%ProgramFiles%\Offboard_Diagnostic_Information_System_Service\OffboardDiagLauncher.exe'
new-netfirewallrule -DisplayName '04 Multicast Kommunikation' -Profile @('Private','Public') -Direction Inbound -Action Allow  -Remoteaddres @('239.255.1.1-239.255.1.2') -protocol 2
new-netfirewallrule -DisplayName '05 DoIP-Broadcast-Kommunikation zwischen ODIS Service und VAS6154(A)' -Profile @('Private','Public') -program '%ProgramFiles%\Offboard_Diagnostic_Information_System_Service\OffboardDiagLauncher.exe' -Direction Inbound -Action Allow  -Remoteaddres @('255.255.255.255')
